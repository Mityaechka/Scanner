{
  "manifest_version": 2,
  "name": "Сканнер",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "declarativeContent"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/scanner.js", "js/chrome.js"]
    }
  ],
  "browser_action": {
    "default_title": "Сканнер",
    "default_popup": "popup/popup.html"
  }
}
